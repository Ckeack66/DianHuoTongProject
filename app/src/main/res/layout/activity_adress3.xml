<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorF7F7F7"
    android:orientation="vertical"
    tools:context="com.mhky.dianhuotong.activity.Adress2Activity">

    <com.mhky.dianhuotong.custom.viewgroup.DianHuoTongBaseTitleBar
        android:id="@+id/adress3_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorF7F7F7">

        <ExpandableListView
            android:id="@+id/addshop3_expandblelistview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorF7F7F7"
            android:divider="@null"
            android:groupIndicator="@null" />

        <com.gjiazhe.wavesidebar.WaveSideBar
            android:id="@+id/addshop3_waveslidebar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:background="@color/colorTransp"
            android:paddingEnd="8dp"
            app:sidebar_lazy_respond="false"
            app:sidebar_max_offset="80dp"
            app:sidebar_position="right"
            app:sidebar_text_alignment="center"
            app:sidebar_text_size="@dimen/t14" />


    </RelativeLayout>


</LinearLayout>

