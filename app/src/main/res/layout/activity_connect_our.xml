<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ymky.dianhuotong.activity.GuoYaoDianActivity">

    <com.ymky.dianhuotong.custom.viewgroup.DianHuoTongBaseTitleBar
        android:id="@+id/connectour_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x64" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/x58"
        android:layout_marginStart="@dimen/x52"
        android:background="@drawable/icon_connect_us"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/x26"
            android:layout_marginTop="@dimen/x76"
            android:background="@color/colorTransp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorTransp"
                android:text="@string/connect_phone"
                android:textColor="@color/color333333"
                android:textSize="@dimen/t12" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorTransp"
                android:text="15212345678"
                android:textColor="@color/color333333"
                android:autoLink="phone"
                android:textColorLink="@color/color04c1ab"
                android:textSize="@dimen/t12" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:layout_marginEnd="@dimen/x26"
            android:layout_marginStart="@dimen/x26"
            android:layout_marginTop="@dimen/x13"
            android:background="@color/colord9d9d9" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/x26"
            android:layout_marginStart="@dimen/x26"
            android:layout_marginTop="@dimen/x12"
            android:background="@color/colorTransp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorTransp"
                android:text="@string/connect_wechat"
                android:textColor="@color/color333333"
                android:textSize="@dimen/t12" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorTransp"
                android:text="ymky02315"
                android:textColor="@color/color333333"
                android:textSize="@dimen/t12" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:layout_marginEnd="@dimen/x26"
            android:layout_marginStart="@dimen/x26"
            android:layout_marginTop="@dimen/x13"
            android:background="@color/colord9d9d9" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/x26"
            android:layout_marginStart="@dimen/x26"
            android:layout_marginTop="@dimen/x12"
            android:background="@color/colorTransp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorTransp"
                android:text="@string/connect_QQ"
                android:textColor="@color/color333333"
                android:textSize="@dimen/t12" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorTransp"
                android:text="96825689"
                android:textColor="@color/color333333"
                android:textSize="@dimen/t12" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:layout_marginEnd="@dimen/x26"
            android:layout_marginStart="@dimen/x26"
            android:layout_marginTop="@dimen/x13"
            android:background="@color/colord9d9d9" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/x26"
            android:layout_marginStart="@dimen/x26"
            android:layout_marginTop="@dimen/x12"
            android:background="@color/colorTransp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorTransp"
                android:text="@string/connect_email"
                android:textColor="@color/color333333"
                android:textSize="@dimen/t12" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/colorTransp"
                android:text="ymky@ymky.com"
                android:textColor="@color/color333333"
                android:autoLink="email"
                android:textColorLink="@color/color04c1ab"
                android:textSize="@dimen/t12" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:layout_marginEnd="@dimen/x26"
            android:layout_marginStart="@dimen/x26"
            android:layout_marginTop="@dimen/x13"
            android:background="@color/colord9d9d9" />
    </LinearLayout>
</LinearLayout>
