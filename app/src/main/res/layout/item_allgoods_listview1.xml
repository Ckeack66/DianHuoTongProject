<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    android:orientation="vertical">
    <LinearLayout
        android:id="@+id/item_all_goods_listview11_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/item_all_goods_listview11_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/x24"
            android:layout_marginTop="@dimen/x24"
            android:text="西药"
            android:textColor="@color/color333333"
            android:textSize="@dimen/t14" />

        <View
            android:id="@+id/item_all_goods_listview11_line"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x0.5"
            android:background="@color/colorBFBFBF" />
    </LinearLayout>

</LinearLayout>