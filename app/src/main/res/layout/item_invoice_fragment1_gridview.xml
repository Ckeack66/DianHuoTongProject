<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/coloreeeeee">
    <ImageView
        android:id="@+id/voice_gridview_item_image"
        android:layout_width="@dimen/x122"
        android:layout_height="@dimen/x85"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/x20"
        android:src="@drawable/invoice_test"/>
    <TextView
        android:id="@+id/voice_gridview_item_txt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="默认证件"
        android:textSize="@dimen/t12"
        android:textColor="@color/color707070"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/x12"
        android:background="@color/colorTransp"/>
    <TextView
        android:id="@+id/voice_gridview_item_txt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(复印件加盖红章)"
        android:textSize="@dimen/t9"
        android:textColor="@color/color707070"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/x9"
        android:background="@color/colorTransp"/>
</LinearLayout>